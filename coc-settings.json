  // CocOutline
  "outline.sortBy": "position",
  "outline.followCursor": true,
  "outline.autoWidth": true,
  "outline.autoPreview": true,
  "outline.expandLevel": 4,
  "list.source.symbols.defaultArgs": ["all"],

  // coc-explorer
  "explorer.git.enable": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.file.reveal.auto": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": "labeling",
  "explorer.enableFloatinput": true,

