    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript.jsx": "typescriptreact",
    "javascript.jsx": "javascriptreact",
    "tsx": "typescript.tsx",
    "jsx": "javascript.tsx",
    "js": "typescriptreact"
